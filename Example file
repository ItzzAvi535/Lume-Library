local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ItzzAvi535/Lume-Library/refs/heads/main/Library", true))()

local window = Library:CreateWindow("Lume Window")

local mainTab = window:AddTab("Main")

mainTab:AddButton("Name of the Button", function()
    -- Your Function here
end)

mainTab:AddLabel("Your text here")

mainTab:AddDropdown("Choose Weapon", {"Sword", "Bow", "Staff"}, function(selectedOption)
    print("Selected option: " .. selectedOption)
end)

mainTab:AddToggle("Toggle name here", function(toggled)
    if toggled then
        print("Feature enabled")
    else
        print("Feature disabled")
    end
end)
